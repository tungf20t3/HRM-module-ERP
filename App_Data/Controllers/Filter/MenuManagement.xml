end
select '' as message, 'UAC.UAC.UpdateLimitReportAccessRight.Update(' + rtrim(@@userID) + ', ' + rtrim(@@userID) + ', ' + '@@appDatabaseName' + ', ' + '@@sysDatabaseName, 2)' as invoke
]]>
