<?xml version="1.0" encoding="utf-8" ?>

<view table="hrnv" code="stt_rec" xmlns="urn:schemas-ai-erp:data-view">
  <fields>
    <field name="image" type="Image" row="4">
      <header v="" e=""></header>
      <clientScript><![CDATA[onclick=show$PanelFullImage(event);]]></clientScript>
    </field>
    <field name="ngay_kham" type="DateTime" dataFormatString="dd/MM/yyyy">
      <header v="Ngày khám" e="Health Care Date"></header>
    </field>
    <field name="chieu_cao" type="Decimal" dataFormatString="#0.00">
      <header v="Chiều cao" e="Height"></header>
    </field>
    <field name="can_nang" type="Decimal" dataFormatString="#000">
      <header v="Cân nặng" e="Weight"></header>
    </field>
    <field name="mat_phai">
      <header v="Mắt phải" e="Right Eye"></header>
    </field>
    <field name="mat_trai">
      <header v="Mắt trái" e="Left Eye"></header>
    </field>
    <field name="nhom_mau">
      <header v="Nhóm máu" e="Blood Type"></header>
    </field>
    <field name="loai_sk">
      <header v="Loại sức khỏe" e="Health Type"></header>
    </field>
    <field name="ghi_chu_sk">
      <header v="Thông tin khác" e="Other Information"></header>
    </field>
  </fields>

  <views>
    <view id="View">
      <item value="71, 79, 70, 50, 80, 70, 10, 70, 200"/>
      <item value="110100: [image], [ngay_kham].Label, [ngay_kham]"/>
      <item value="+10100: [chieu_cao].Label, [chieu_cao]"/>
      <item value="+10100: [can_nang].Label, [can_nang]"/>
      <item value="+10100: [nhom_mau].Label, [nhom_mau]"/>
      <item value="-"/>
      <item value="10100: [mat_phai].Label, [mat_phai]"/>
      <item value="10100: [mat_trai].Label, [mat_trai]"/>
      <item value="10100: [loai_sk].Label, [loai_sk]"/>
      <item value="10100000: [ghi_chu_sk].Label, [ghi_chu_sk]"/>
      <item value="-"/>
    </view>
  </views>

</view>