<queries>
  <query event="Loading">
    <items>
      <item source="&Recognize;" destination="&Control.Field.Voucher.Join.User.Created;&Control.Field.Voucher.Join.User.Modified;&Recognize;"/>
    </items>
  </query>

  <query event="Finding">
    <items>
      <item source="&Recognize;" destination="&Control.Field.Voucher.Join.User.Created;&Control.Field.Voucher.Join.User.Modified;&Recognize;"/>
    </items>
  </query>

  <query event="Scattering">
    <clauses>
      <clause type="From" statement=" a&Control.Field.Voucher.Join.User.Created;&Control.Field.Voucher.Join.User.Modified;"/>
    </clauses>
    <items>
      &Control.Field.Voucher.Item.Date.Created;
      &Control.Field.Voucher.Item.Date.Modified;
      &Control.Field.Voucher.Item.User.Created;
      &Control.Field.Voucher.Item.User.Modified;
      &Control.Field.Voucher.Item.Identity;
    </items>
  </query>
</queries>