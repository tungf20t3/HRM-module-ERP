<?xml version="1.0" encoding="utf-8"?>

<grid table="hrctvt" code="ma_vtr" order="ma_vtr, line_nbr" xmlns="urn:schemas-ai-erp:data-grid">
  <title v="" e=""></title>
  <subTitle v="" e=""></subTitle>

  <fields>
    <field name="ma_cv" dataFormatString="@upperCaseFormat" width="80" allowNulls="false">
      <header v="Mã công việc" e="Job"></header>
      <items style="Mask"/>
    </field>
    <field name="ten_cv" width="150" allowNulls="false">
      <header v="Tên công việc" e="Description"></header>
    </field>
    <field name="ten_cv2" width="150">
      <header v="Tên khác" e="Other Name"></header>
    </field>
    <field name="ghi_chu" width="300">
      <header v="Ghi chú" e="Note"></header>
    </field>
    <field name="line_nbr" type="Int32" isPrimaryKey="true" width="0" align="right" hidden="true">
      <header v="" e=""></header>
    </field>
  </fields>

  <views>
    <view id="Grid">
      <field name="ma_cv"/>
      <field name="ten_cv"/>
      <field name="ten_cv2"/>
      <field name="ghi_chu"/>
      <field name="line_nbr"/>
    </view>
  </views>

  <toolbar>
    <button command="Insert">
      <title v="Toolbar.Insert" e="Toolbar.Insert"></title>
    </button>
    <button command="Grow">
      <title v="Toolbar.Grow" e="Toolbar.Grow"></title>
    </button>
    <button command="Down">
      <title v="Toolbar.Down" e="Toolbar.Down"></title>
    </button>
    <button command="Clone">
      <title v="Toolbar.Clone" e="Toolbar.Clone"></title>
    </button>
    <button command="Remove">
      <title v="Toolbar.Remove" e="Toolbar.Remove"></title>
    </button>

    <button command="Freeze">
      <title v="Toolbar.Freeze" e="Toolbar.Freeze"></title>
    </button>
  </toolbar>
</grid>