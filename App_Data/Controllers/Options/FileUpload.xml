<?xml version="1.0" encoding="utf-8"?>
<upload xmlns="urn:schemas-ai-erp:file-upload">
  <data id="ReportLogo" type="Image">
    <setting>
      <maxFileSize value="5" />
      <allowLogFile value="false" />
      <uploadTimeOut value="60" />
      <processDelayTime value="20" />
      <defaultFileName value="Logo.png" />
      <savedFilePath value="Images\Report" />
      <allowFileExtension value="^(.GIF|.PNG|.JPE?G)$" />
      <invokeMethod value="SetImageDefault($t,$k,$d)" />
    </setting>
    <log>
    </log>
  </data>
  <data id="EmployeeImage" type="Image">
    <setting>
      <thumbWidth value="60" />
      <thumbHeight value="80" />
      <maxFileSize value="5" />
      <uploadTimeOut value="120" />
      <processDelayTime value="500" />
      <imageRecordTimeOut value="1740" />
      <allowFileExtension value="^(.GIF|.PNG|.JPE?G)$" />
    </setting>
  </data>
  <data id="ProfileImage" type="Image">
    <setting>
      <thumbWidth value="80" />
      <thumbHeight value="80" />
      <maxFileSize value="1" />
      <uploadTimeOut value="120" />
      <processDelayTime value="500" />
      <imageRecordTimeOut value="1740" />
      <allowFileExtension value="^(.GIF|.PNG|.JPE?G)$" />
    </setting>
  </data>
  <data id="Default" type="Excel">
    <setting>
      <maxFileSize value="100" />
      <uploadTimeOut value="60" />
      <processDelayTime value="100" />
      <savedFilePath value="App_Data\Controllers\Templates\Excel" />
      <uploadContentType value="application/ms-excel;application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
      <dowloadContentType value="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet" />
      <dataRecordTimeOut value="1740" />
      <allowFileExtension value="^(.XLSX)$" />
      <onProcessFail value="parent.on$Upload$Fail(this.frameElement)"/>
      <onProcessComplete value="parent.on$Upload$Complete(this.frameElement)"/>
    </setting>
  </data>
  <data id="PaymentForm" type="Rpt">
    <setting>
      <maxFileSize value="1" />
      <uploadTimeOut value="60" />
      <processDelayTime value="100" />
      <savedFilePath value="App_Data\Controllers\Templates\Rpt" />
      <uploadContentType value="application/rpt" />
      <dowloadContentType value="application/rpt" />
      <dataRecordTimeOut value="1740" />
      <allowFileExtension value="^(.RPT)$" />
      <onProcessFail value="parent.on$PaymentFormUpload$Fail(this.frameElement)"/>
      <onProcessComplete value="parent.on$PaymentFormUpload$Complete(this.frameElement)"/>
    </setting>
  </data>
  <data id="DataCopy" type="Zip">
    <setting>
      <maxFileSize value="1" />
      <uploadTimeOut value="60" />
      <processDelayTime value="100" />
      <defaultFile value="0" />
      <savedFilePath value="Upload\Transfer\Import" />
      <uploadContentType value="application/zip" />
      <dowloadContentType value="application/zip" />
      <dataRecordTimeOut value="1740" />
      <allowFileExtension value="^(.ZIP)$" />
      <onProcessFail value="parent.on$DataCopyUpload$Fail(this.frameElement)" />
      <onProcessComplete value="parent.on$DataCopyUpload$Complete(this.frameElement)" />
    </setting>
  </data>
  <data id="DataCopyExport" type="Zip">
    <setting>
      <maxFileSize value="1" />
      <uploadTimeOut value="60" />
      <processDelayTime value="100" />
      <savedFilePath value="Upload\Transfer\Export" />
      <uploadContentType value="application/zip" />
      <dowloadContentType value="application/zip" />
      <dataRecordTimeOut value="1740" />
      <allowFileExtension value="^(.ZIP)$" />
      <onProcessFail value="parent.on$DataCopyUpload$Fail(this.frameElement)" />
      <onProcessComplete value="parent.on$DataCopyUpload$Complete(this.frameElement)" />
    </setting>
  </data>
</upload>