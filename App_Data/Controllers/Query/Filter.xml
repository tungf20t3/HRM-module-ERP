<?xml version="1.0" encoding="utf-8"?>

<query xmlns="urn:schemas-ai-erp:data-query">
  <filter>
    <operators type="Comparison">
      <operator id="c1" value="=" blank="true" dataType="String,DateTime,Numeric">
        <title v="Bằng" e="Equal"/>
      </operator>
      <operator id="c2" value="&lt;&gt;" dataType="String,DateTime,Numeric">
        <title v="Khác" e="Not Equal"/>
      </operator>
      <operator id="c3" value="&lt;" dataType="String,DateTime,Numeric">
        <title v="Nhỏ hơn" e="Less than"/>
      </operator>
      <operator id="c4" value="&gt;" dataType="String,DateTime,Numeric">
        <title v="Lớn hơn" e="Greater than"/>
      </operator>
      <operator id="c5" value="&lt;=" dataType="String,DateTime,Numeric">
        <title v="Nhỏ hơn hoặc bằng" e="Less or Equal"/>
      </operator>
      <operator id="c6" value="&gt;=" dataType="String,DateTime,Numeric">
        <title v="Lớn hơn hoặc bằng" e="Greater or Equal"/>
      </operator>
      <operator id="c7" value="like" dataType="String">
        <title v="Bắt đầu bằng" e="Starts with"/>
      </operator>
      <operator id="c10" value="=" dataType="Boolean">
        <title v="Đúng" e="True"/>
      </operator>
      <operator id="c11" value="&lt;&gt;" dataType="Boolean">
        <title v="Sai" e="False"/>
      </operator>
    </operators>

    <operators type="Logic">
      <operator id="l1" value="and" blank="true">
        <title v="&lt;span style=&quot;color:#c3c3c3;&quot;&gt;Trắng&lt;/span&gt;" e="&lt;span style=&quot;color:#c3c3c3;&quot;&gt;None&lt;/span&gt;"/>
      </operator>
      <operator id="l2" value="and">
        <title v="Và" e="And"/>
      </operator>
      <operator id="l3" value="or">
        <title v="Hoặc" e="Or"/>
      </operator>
      <operator id="l4" value="and" priority="1">
        <title v="Và mở ngoặc" e="And Open Bracket"/>
      </operator>
      <operator id="l5" value="or" priority="1">
        <title v="Hoặc mở ngoặc" e="Or Open Bracket"/>
      </operator>
      <operator id="l6" value="and" priority="2">
        <title v="Và &lt;span style=&quot;color:#c3c3c3;&quot;&gt;&lt;...&gt;&lt;/span&gt; đóng ngoặc" e="And &lt;span style=&quot;color:#c3c3c3;&quot;&gt;&lt;...&gt;&lt;/span&gt; Closed Bracket"/>
      </operator>
      <operator id="l7" value="or" priority="2">
        <title v="Hoặc &lt;span style=&quot;color:#c3c3c3;&quot;&gt;&lt;...&gt;&lt;/span&gt; đóng ngoặc" e="Or &lt;span style=&quot;color:#c3c3c3;&quot;&gt;&lt;...&gt;&lt;/span&gt; Closed Bracket"/>
      </operator>
    </operators>
  </filter>
</query>