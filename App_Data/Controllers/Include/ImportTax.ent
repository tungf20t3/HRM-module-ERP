  <!ENTITY DetailTaxUpdateMasterValue "">

  <!ENTITY ARImportTaxUpdate "&ImportTaxUpdate.DiscountRate;">
  <!ENTITY SVTranGLCOGS.ext "">
  <!ENTITY SVImportTaxUpdate "&ImportTaxUpdate.DiscountRate;&ImportTaxUpdateLocationLot;">
]]>


