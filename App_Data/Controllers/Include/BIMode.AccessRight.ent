g._hiddenForms = [
  {Form: [0, 1, 4, 5], Value: (t == '4')},
  {Form: [1, 2, 5, 6], Value: (t == '1' || t == '2' || t == '5')},
  {Form: [0, 2, 4, 6], Value: (t == '3')}
];
">
