$dt.aExpression.push('[t_thue_km_nt]:=[t_tien_km_nt]*[thue_suat]/100', '[t_thue_km]:=[t_tien_km]*[thue_suat]/100', '[t_km_nt]:=[t_tien_km_nt]+[t_thue_km_nt]', '[t_km]:=[t_tien_km]+[t_thue_km]');
">
