  <!ENTITY ManagementObjectChange "onchange=&quot;onChange$&Controller;$Object(this);&quot;">
  <!ENTITY ManagementObjectScriptActive "onChange$&Controller;$Object(f.getItem('loai_dtqt'););">

  <!ENTITY ManagementObjectIndex "">
