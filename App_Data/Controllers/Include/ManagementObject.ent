function onChange$&Controller;$Object(o) {
  var f = o.parentForm;
  f.setReferenceKeyFilter('ma_dtqt');
  f.setReferenceKeyFilter('nh_dtqt1');
  f.setReferenceKeyFilter('nh_dtqt2');
  f.setReferenceKeyFilter('nh_dtqt3');
}">
