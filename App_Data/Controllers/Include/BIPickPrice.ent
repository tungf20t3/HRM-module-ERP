<!ENTITY PickPriceButton "">
<!ENTITY PickPriceMenuItem "">
<!ENTITY PickPriceConditionShow "">
<!ENTITY PickPriceExecuteShow "">
<!ENTITY PickPriceRetrieveFunction "">
<!ENTITY PickPriceParentController "">
<!ENTITY PickPriceParentGridField "">