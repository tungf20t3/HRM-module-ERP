        <header v="Biên bản kiểm kê" e="Stocktaking Sheets"/>
      </download>
      <fields>
        <field name="h_dl_yn" type="Boolean">
          <header v="1" e="1"/>
        </field>
      </fields>
    </form>

    &s;

