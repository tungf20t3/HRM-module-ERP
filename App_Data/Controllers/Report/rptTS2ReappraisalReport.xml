    <field name="chairman" type="String">
      <header v="CHỦ TỊCH HỘI ĐỒNG" e="CHAIRMAN"/>
    </field>
    <field name="trustee" type="String">
      <header v="ỦY VIÊN/NGƯỜI LẬP" e="TRUSTEE/PREPARED"/>
    </field>
  </fields>
</report>