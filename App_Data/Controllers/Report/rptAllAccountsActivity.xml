        <header v="Sổ chi tiết tài khoản dạng song ngữ" e="Account Activity - Bilingual Form"/>
      </download>&b;
    </form>

    <form id="040" reportFile ="rptAllAccountsActivity_01FCBI" commandArgument="pdf" languageType="0">
