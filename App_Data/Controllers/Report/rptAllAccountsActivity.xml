      <download>
        <header v="Sổ chi tiết tài khoản tiền ngoại tệ" e="Account Activity in Foreign Currency"/>
      </download>&f;
    </form>

    &s;

    <form id="030" reportFile ="rptAllAccountsActivity_01BI" commandArgument="pdf" languageType="0" urlImage="&bi;">
