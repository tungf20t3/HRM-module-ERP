    </field>
    <field name="h_ng_nt" type="String">
      <header v="Nguyên giá nt" e="FC Original Cost"/>
    </field>
    <field name="h_ngay" type="String">
      <header v="Ngày:" e="Date:"/>
    </field>
    <field name="h_ngay_bi" type="String">
